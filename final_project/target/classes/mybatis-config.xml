<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<!-- null 값을 임의로 허용해주기 -->
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<!-- type=full name, alias 별칭 -->
		<typeAlias type="com.gun.board.vo.Customer" alias="Customer" />
		<typeAlias type="com.gun.board.vo.Board" alias="Board" />
		<typeAlias type="com.gun.board.vo.Reply" alias="Reply" />
		<typeAlias type="com.gun.board.vo.Message" alias="Message" />
		<typeAlias type="com.gun.board.vo.Notice" alias="Notice" />
		<typeAlias type="com.gun.board.vo.Free" alias="Free" />
		<typeAlias type="com.gun.board.vo.Data" alias="Data" />
		<typeAlias type="com.gun.board.vo.Reply_Data" alias="Reply_Data" />
		<typeAlias type="com.gun.board.vo.Reply_Free" alias="Reply_Free" />
		<typeAlias type="com.gun.board.vo.Auction" alias="Auction" />
		<typeAlias type="com.gun.board.vo.Friend" alias="Friend" />
	</typeAliases>
	
</configuration>